import numpy as np

# Example 3x3 matrix: each row is a product, each column a sale price
sales_matrix = np.array([
    [100, 120, 110],  # Product 1 sales
    [200, 210, 190],  # Product 2 sales
    [50,  60,  55]    # Product 3 sales
])

# Calculate the average price of all products sold
average_price = np.mean(sales_matrix)

print(f"Average price of all products sold in the past month: {average_price}")
